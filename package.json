    "lint-staged": "^14.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run format -- --write",
      "npm run test -- --changed"
    ],
    "*.rs": [
      "cargo fmt --all -- --check"
