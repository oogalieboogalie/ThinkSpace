    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.0.0",
    "@types/three": "^0.181.0",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.0",
    "katex": "^0.16.27",
    "lucide-react": "^0.292.0",
    "manifold-3d": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
